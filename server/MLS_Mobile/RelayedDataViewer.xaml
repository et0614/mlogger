<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:MLS_Mobile"
             x:Class="MLS_Mobile.RelayedDataViewer"
             Title="RelayedDataViewer">
    <CollectionView x:Name="mlvList" ItemsSource="{ Binding MLoggerViewModelList }" SelectionMode="Single" SelectionChanged="mlvList_SelectionChanged">
        <CollectionView.ItemTemplate>
            <DataTemplate>
                <local:MLoggerView Padding="10"/>
            </DataTemplate>
        </CollectionView.ItemTemplate>
    </CollectionView>
</ContentPage>